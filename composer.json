{
    "name": "jungwild/crawler",
    "description": "builds jungwild crawler projects",
    "license": "MIT",
    "type": "project",
    "require": {
        "wa72/htmlpagedom": "^1.3",
        "guzzlehttp/guzzle": "^6.3",
        "vlucas/phpdotenv": "^2.5"
    },
    "autoload":{
        "psr-4":{
            "Jungwild\\":"src/"
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('_env_example', '.env');\""
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
